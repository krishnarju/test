pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
 checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'git', url: 'https://github.com/krishnarju/test.git']])
              
            }
        }
        stage('build') {
            steps {
               sh 'mvn -DskipTests clean package'
            }
        }
stage('sonarscanner') {
            steps {
              
  withSonarQubeEnv('sonarqube-8.9') {
 sh "mvn sonar:sonar"
    }
}
            }
        }

      stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }

